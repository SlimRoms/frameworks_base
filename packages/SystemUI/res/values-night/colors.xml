<resources xmlns:android="http://schemas.android.com/apk/res/android">
    <color name="system_primary_color">@*android:color/material_grey_850</color>
    <color name="system_secondary_color">@*android:color/material_grey_800</color>
    <color name="system_accent_color">@*android:color/material_deep_teal_500</color>
    <color name="qs_detail_transition">@*android:color/material_grey_850</color>

    <!-- The "inside" of a notification, reached via longpress -->
    <color name="notification_guts_bg_color">#FF101315</color>
    <color name="notification_guts_title_color">#FFFFFFFF</color>
    <color name="notification_guts_text_color">#b2FFFFFF</color>
    <color name="notification_guts_btn_color">#FFFFFFFF</color>

    <!-- The color of the legacy notification background -->
    <color name="notification_legacy_background_color">@*android:color/material_grey_850</color>
    <!-- The color of the material notification background -->
    <color name="notification_material_background_color">@*android:color/material_grey_850</color>
    <!-- The color of the material notification background when dimmed -->
    <color name="notification_material_background_dimmed_color">@*android:color/material_grey_850</color>
    <!-- The color of the material notification background when low priority -->
    <color name="notification_material_background_low_priority_color">@*android:color/material_grey_850</color>
    <!-- The color of the material notification background for media notifications when no custom
         color is specified -->
    <color name="notification_material_background_media_default_color">@*android:color/material_grey_850</color>

    <color name="action_icon_color">#FFFFFFFF</color>

</resources>
